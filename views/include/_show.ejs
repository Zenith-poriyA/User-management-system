<% for(var i=0;i<users.length;i++){%>
<tr>
    <th scope="row"><%= i+1 %></th>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a class="btn btn-primary btn-sm bg-warning border-warning text-white" href="/update_user?id=<%= users[i]._id %>" role="button" title="Edit"><i class="fas fa-edit"></i></a>
        <a class="btn btn-primary btn-sm bg-danger border-danger text-white delete" data-id=<%= users[i]._id %> role="button" title="Delete"><i class="fas fa-trash-alt"></i></a>
    </td>
</tr>
<% } %>